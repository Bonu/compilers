
Tokens
	number = ['0'..'9']+;
	plus = '+'; 
	minus = '-';
	l_par = '('; 
	r_par = ')';
	blank = ' ';
	
Ignored Tokens 
	blank;
	
Productions
	expr = {plus} expr plus number 
		| {minus} expr minus number 
		| {par} l_par expr r_par 
		| {number} number 
		| ( {op} [left]:expr op [right]:expr );
	
	op = ( {plus} plus ) 
		| ( {minus} minus ); 

